FUNCTION binarySearch(sorted, value)
    DECLARE start = 0
    DECLARE end = length(sorted)
    DECLARE half
    WHILE start != end
        SET half = halfway between start and end
        IF sorted[half] = value
            return half
        ENDIF
        ELSE IF sorted[half] > Value
            SET start = half
        ENDIF
        ELSE
            SET END = half
        ENDELSE
    ENDWHILE
    RETURN NONE
ENDFUNCTION